snippet alias "Custom alias"
alias ${1:name}="${2:cmd}"
endsnippet

snippet 1lf "One-line function"
${1:name}() { ${2:cmd} ; } 
endsnippet

snippet mlf "Multi-line function"
${1:name}() { 
	${2:cmd} 
} 
endsnippet

snippet evr "Var with escaped quotes" i
\"$${1:var}\"
endsnippet

snippet vr "Var with quotes" i
"$${1:var}"
endsnippet

